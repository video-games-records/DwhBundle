{
    "name": "video-games-records/dwh-bundle",
    "description": "VideoGamesRecords DwhBundle",
    "type": "symfony-bundle",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Benard David",
            "email": "magicbart@gmail.com",
            "homepage": "https://www.vgr-magicbart.com/",
            "role": "Developper"
        }
    ],
    "require": {
        "php": "^8.0",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "~2.0",
        "symfony/config": "^5.4 || ^6.0",
        "symfony/console": "^5.4 || ^6.0",
        "symfony/dependency-injection": "^5.4 || ^6.0",
        "symfony/http-kernel": "^5.4 || ^6.0",
        "video-games-records/core-bundle": "*",
        "projet-normandie/article-bundle": "*"
    },
    "suggest": {
        "doctrine/doctrine-fixtures-bundle": "To be able to load fixtures"
    },
    "autoload": {
        "psr-4": {
            "VideoGamesRecords\\DwhBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VideoGamesRecords\\DwhBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
